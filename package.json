{
  "name": "off-ended-productions",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "seeds": "node ./bin/runAllSeeds.js",
    "getDataFeeds": "node bin/getData.js",
    "watch:sass": "sass --watch --style=compressed ./public/css/main.sass ./public/css/main.css",
    "tailwind": "tailwindcss --config tailwind.config.js -i ./public/css/main.css -o ./public/css/main.min.css --watch",
    "mongoDB:start": "sudo systemctl start mongod.service"
  },
  "dependencies": {
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "mongoose": "^8.9.5",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^8.0.0"
  },
  "devDependencies": {
    "glob": "^11.0.1",
    "nodemon": "^3.1.9",
    "sass": "^1.83.4",
    "tailwindcss": "^3.4.15"
  }
}
